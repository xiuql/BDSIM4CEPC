nEmitX    = 5;
nEmitY    = 5;
nEnvelX   = 5;
nEnvelY   = 5;
emitX     = 0.05*m;
emitY     = 0.05*m;
emitHaloX = nEmitX*emitX;
emitHaloY = nEmitY*emitY;
betX      = 0.6*m;
betY      = 0.6*m;
alfX      = -1.0;
alfY      = 1.0;
gamX      = (1+alfX*alfX)/betX;
gamY      = (1+alfY*alfY)/betY;
maxX      = sqrt(emitHaloX*betX);
maxXp     = sqrt(emitHaloX*gamX);
maxY      = sqrt(emitHaloY*betY);
maxYp     = sqrt(emitHaloY*gamY);
envelX    = nEnvelX*maxX;
envelXp   = nEnvelX*mapXp;
envelY    = nEnvelY*maxY;
envelYp   = nEnvelY*maxYp;

beam, particle="e-",
      energy=1.0*GeV,
      distrType="halo",
      betx=betX,
      bety=betY,
      alfx=alfX,
      alfy=alfY,
      emitx=emitHaloX,
      emity=emitHaloY,
      sigmaE=0.00008836,
      sigmaT=0.00000000001,
      envelopeX  = envelX,	
      envelopeY  = envelY, 
      envelopeXp = envelXp,
      envelopeYp = envelYp,
      haloPSWeightParameter=1,
      haloPSWeightFunction="oneoverr";

