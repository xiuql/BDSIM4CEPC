TITLE,'Example 3: DRIFT.MADX';

BEAM, PARTICLE=PROTON,ENERGY=10.0, EX=1e-6, EY=1e-6;

DR: DRIFT,L=2.5;

DRIFT: LINE=(DR);
USE, PERIOD=DRIFT;

SELECT, FLAG=TWISS,CLEAR;

TWISS,SAVE,BETX=1.5,BETY=1.5, FILE=DRIFT.tfs;
PLOT,HAXIS=S,VAXIS=BETX,BETY;

SAVEBETA, LABEL=start, PLACE=DR, SEQUENCE=DRIFT;

Value, TABLE(SUMM,Q1);
Value, TABLE(SUMM,Q2);
WRITE,TABLE=SUMM,FILE=print.dat;

