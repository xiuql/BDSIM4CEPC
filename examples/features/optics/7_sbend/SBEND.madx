TITLE,'Example 5: SBEND.MADX';

BEAM, PARTICLE=PROTON,ENERGY=10.0,SIGE=0.001,EX=5e-7, EY=5e-7;

SB: SBEND,L=1.0,ANGLE=0.001;

SBEND: LINE=(SB);
USE, PERIOD=SBEND;

SELECT, FLAG=TWISS,CLEAR;

TWISS,SAVE,BETX=12.0,BETY=12.0,ALFX=0.1,ALFY=0.1,FILE=SBEND.tfs;
PLOT,HAXIS=S,VAXIS=BETX,BETY;
PLOT,HAXIS=S,VAXIS=ALFX,ALFY;

SAVEBETA, LABEL=start, PLACE=SB, SEQUENCE=SBEND;



Value, TABLE(SUMM,Q1);
Value, TABLE(SUMM,Q2);
WRITE,TABLE=SUMM,FILE=print.dat;

