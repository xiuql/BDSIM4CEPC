//MADX Example4: SEXT.MADX


TITLE,'Example 4: SEXT.MADX';

BEAM, PARTICLE=PROTON,ENERGY=10.0, EX=1e-6, EY=1e-6;

//A very long and strong sextupole
SX: SEXTUPOLE,L=0.7,K2=1300.0;

SEXT: LINE=(SX);
USE, PERIOD=SEXT;

SELECT, FLAG=TWISS,CLEAR;

TWISS,SAVE,BETX=12.0,BETY=6.0,ALFX=0.1,ALFY=0.1,FILE=SEXT.tfs;
PLOT,HAXIS=S,VAXIS=BETX,BETY;
PLOT,HAXIS=S,VAXIS=ALFX,ALFY;

SAVEBETA, LABEL=start, PLACE=SX, SEQUENCE=SEXT;



Value, TABLE(SUMM,Q1);
Value, TABLE(SUMM,Q2);
WRITE,TABLE=SUMM,FILE=print.dat;

