include optionsBiasing.gmad

option, xsecBias, logicalVolumes="acceleratorVacuum", bias="biasDef1";
option, xsecBias, logicalVolumes="componentMaterial", bias="biasDef2"; 

d1 : drift, l=1*m;
q1 : quadrupole, l=1*m, material="Iron" ! uses the process biasDef
d2 : drift, l=1*m;

beamLine: line = (d1,q1,d2); !defines the beam line
use, period=beamLine; !Chooses the beam line to use

option, ngenerate=100;