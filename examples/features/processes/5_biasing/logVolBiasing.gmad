include optionsBiasing.gmad

biasDef1: xsecBias, particle="e-", proc="all", xsecfact=10, flag=3;
biasDef2: xsecBias, particle="e+", proc="eBrem eIoni msc", xsecfact={10,1,5}, flag={1,1,2};
!biasDef2: xsecBias, particle="e+", proc="eBrem eIoni msc", xsecfact={10,1,5}, flag={1,1,2};

d1 : drift, l=1*m;
q1 : quadrupole, l=1*m, material="Iron";
d2 : drift, l=1*m;

beamLine: line = (d1,q1,d2); !defines the beam line
use, period=beamLine; !Chooses the beam line to use

option, ngenerate=100;