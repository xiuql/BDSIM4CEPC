d1: drift, l=0.2*m, apertureType="racetrack", aper1=2*cm, aper2=1*cm, aper3=2.4*cm, beampipeThickness=5*mm;
sb1: sbend, l=0.3*m, angle=0.1, apertureType="racetrack", aper1=2*cm, aper2=1*cm, aper3=2.4*cm, beampipeThickness=5*mm;

endoftheline: marker;

l1: line = (d1, sb1, d1, endoftheline);

use, period = l1;

sample, range=endoftheline;

include options.gmad;
